  "about": {
   "description": "This library contains an add-on to FEniCSx enabling the checkpointing meshes and solutions from DOLFINx\n",
   "dev_url": "https://github.com/jorgensd/adios4dolfinx/",
   "doc_url": "https://jorgensd.github.io/adios4dolfinx/",
   "home": "https://github.com/jorgensd/adios4dolfinx",
   "license": "MIT",
   "license_family": "MIT",
   "license_file": "LICENSE",
   "summary": "Checkpointing for DOLFINx"
  },
  "build": {
   "noarch": "python",
   "number": "0",
   "script": "python -m pip install . -vv",
   "script_env": [
    "OMPI_ALLOW_RUN_AS_ROOT=1",
    "OMPI_ALLOW_RUN_AS_ROOT_CONFIRM=1",
    "OMPI_MCA_rmaps_base_oversubscribe=1",
    "OMPI_MCA_plm=isolated",
    "OMPI_MCA_btl=vader,self",
    "OMPI_MCA_btl_vader_single_copy_mechanism=none",
    "MPIEXEC_TIMEOUT=400"
   ]
  },
  "extra": {
   "feedstock-name": "adios4dolfinx",
   "recipe-maintainers": [
    "jorgensd",
    "minrk"
   ]
  },
  "package": {
   "name": "adios4dolfinx",
   "version": "0.8.1"
  },
  "requirements": {
   "host": [
    "python >=3.9",
    "pip"
   ],
   "run": [
    "fenics-dolfinx 0.8",
    "adios2 * mpi_*",
    "numpy >=1.21",
    "python >=3.9",
    "mpi4py"
   ]
  },
  "source": {
   "sha256": "5e1ba02f2cdebe22193d6134684e4b733e500f07146e1a84cb8aaa329098dbba",
