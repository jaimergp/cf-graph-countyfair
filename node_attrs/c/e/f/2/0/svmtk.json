  "about": {
   "description": "Surface Volume Meshing Toolkit (SVMTK) is a Python frontend\nfor specific 2D and 3D mesh generation methods in the\nComputational Geometry Algorithms Library (CGAL).\n\nSVMTK was designed to create volume meshes of soft organic tissue surfaces,\nlike the human brain, with the option to repair unphysical errors on the surfaces.\n",
   "dev_url": "https://github.com/SVMTK/SVMTK",
   "home": "https://github.com/SVMTK/SVMTK",
   "license": "GPL-3.0-only",
   "license_family": "GPL",
   "license_file": "LICENSE",
   "summary": "Surface Volume Meshing Toolkit (SVMTK)"
  },
  "build": {
   "number": "3",
   "script": "PYTHON -m pip install -vv --ignore-installed --no-deps .",
   "script_env": [
    "CMAKE_DOWNLOAD_CGAL=OFF",
    "CMAKE_DOWNLOAD_PYBIND11=OFF",
    "CMAKE_BUILD_TESTING=OFF"
   ]
  },
  "extra": {
   "recipe-maintainers": [
