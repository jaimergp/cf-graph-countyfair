   "url": "https://pypi.io/packages/source/i/imagecodecs/imagecodecs-2024.6.1.tar.gz"
  },
  "test": {
   "commands": [
    "echo \"Testing for BLOSC\"",
    "python -c \"import imagecodecs; assert imagecodecs.BLOSC.available\"",
    "echo \"Testing for BLOSC2\"",
