    "petsc * complex_*"
   ],
   "script_env": [
    "CUDA_CONDA_TARGET_NAME="
   ],
   "string": "cuda11_complex_h1234567_1",
   "track_features": [
    "petsc_complex",
    "petsc_cuda"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "jan-janssen",
    "dalcinl",
