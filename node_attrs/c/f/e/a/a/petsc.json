    "SylvainCorlay",
    "martinRenou",
    "matthiasdiener",
    "stefanozampini"
   ]
  },
  "package": {
   "name": "petsc",
   "version": "3.21.2"
  },
  "requirements": {
   "build": [
    "c_compiler_stub",
    "c_stdlib_stub",
    "cxx_compiler_stub",
    "fortran_compiler_stub",
    "cuda_compiler_stub",
    "make",
    "pkg-config",
    "python",
    "gnuconfig"
   ],
   "host": [
    "libblas",
    "libcblas",
    "liblapack",
    "cmake",
    "openmpi",
    "yaml",
    "hypre",
    "metis",
    "parmetis",
    "libptscotch",
    "scalapack",
    "superlu",
    "superlu_dist",
    "libscotch",
    "mumps-mpi",
    "suitesparse",
    "hdf5",
    "hdf5 * mpi_openmpi_*",
    "fftw * mpi_openmpi_*",
    "cuda-version 11.8",
    "cudatoolkit",
    "cuda-version 12.4"
   ],
   "run": [
    "openmpi",
    "yaml",
    "hypre",
    "metis",
    "parmetis",
    "libptscotch",
    "scalapack",
    "superlu",
    "superlu_dist",
    "libscotch",
    "mumps-mpi",
    "suitesparse",
    "hdf5",
    "hdf5 * mpi_openmpi_*",
    "fftw * mpi_openmpi_*",
    "cuda-version >=11.2,<12",
    "cudatoolkit >=11.2,<12"
   ]
  },
  "source": {
   "patches": [
    "ignore-not-invalid.patch",
    "no-cppflags-in-pkgconfig-cflags.patch",
    "target-cudadir.patch"
   ],
   "sha256": "a1ac62b6204bdf2f7f9b637abf45e6cff24d372d4d3d3702c50e157bdb56eb21",
   "url": "https://web.cels.anl.gov/projects/petsc/download/release-snapshots/petsc-3.21.2.tar.gz"
  },
  "test": {
   "commands": [
    "pkg-config --validate PETSc",
    "pkg-config --cflags PETSc",
    "pkg-config --libs PETSc"
   ],
   "files": [
    "tests/ex1.c",
    "tests/ex1f.F90",
    "tests/testdlopen.c",
    "tests/makefile"
   ],
   "requires": [
    "pkg-config",
    "make",
    "c_compiler_stub",
    "cxx_compiler_stub",
    "fortran_compiler_stub"
   ]
  }
 },
