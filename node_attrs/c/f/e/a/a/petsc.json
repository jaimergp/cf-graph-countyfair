   "license_file": "LICENSE",
   "summary": "PETSc: Portable, Extensible Toolkit for Scientific Computation"
  },
  "build": {
   "ignore_run_exports_from": [
    "cuda_compiler_stub"
   ],
   "number": "1",
   "run_exports": [
