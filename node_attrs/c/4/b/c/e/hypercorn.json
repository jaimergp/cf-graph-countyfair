 "linux_64_meta_yaml": {
  "about": {
   "description": "Hypercorn is an ASGI web server based on the sans-io hyper, h11, h2,\nand wsproto libraries and inspired by Gunicorn. Hypercorn supports HTTP/1, HTTP/2,\nand websockets and the ASGI 2 specification. Hypercorn can utilise asyncio,\nuvloop, or trio worker types.\n",
   "dev_url": "https://gitlab.com/pgjones/hypercorn/",
   "doc_url": "https://pgjones.gitlab.io/hypercorn/",
   "home": "https://gitlab.com/pgjones/hypercorn/",
   "license": "MIT",
   "license_family": "MIT",
   "license_file": "LICENSE",
   "summary": "A ASGI Server based on Hyper libraries and inspired by Gunicorn."
  },
  "build": {
   "entry_points": [
    "hypercorn = hypercorn.__main__:main"
   ],
   "number": "2",
   "script": "PYTHON -m pip install . --no-deps -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "maxyme",
    "dhirschfeld",
    "synapticarbors"
   ]
  },
  "package": {
   "name": "hypercorn",
   "version": "0.14.3"
  },
  "requirements": {
   "host": [
    "pip",
    "python",
    "setuptools",
    "poetry-core >=1.0.0"
   ],
   "run": [
    "python",
    "h11 >=0.8.1",
    "h2 >=3.1.0",
    "priority",
    "toml >=0.10.0",
    "typing_extensions >=3.7.4",
    "wsproto >=0.14.0",
    "uvloop >=0.11.3",
    "trio >=0.11.0"
   ]
  },
  "source": {
   "fn": "Hypercorn-0.14.3.tar.gz",
