   "license_family": "BSD",
   "license_file": "LICENSE",
   "summary": "Python wrapper for IPOPT"
  },
  "build": {
   "number": "1",
   "script": [
    "export CXXFLAGS=\"${CXXFLAGS} -D_LIBCPP_DISABLE_AVAILABILITY\"",
    "export CFLAGS=$CXXFLAGS",
    "export CC=$CXX",
    "PYTHON -m pip install --no-deps . -vv"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "traversaro",
    "minrk"
   ]
  },
