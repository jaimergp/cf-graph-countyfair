 "feedstock_name": "cwltest",
 "hash_type": "sha256",
 "linux_64_meta_yaml": {
  "about": {
   "description": "This is a testing tool for checking the output of Tools and Workflows described\nwith the Common Workflow Language.  Among other uses, it is used to run the CWL\nconformance tests.\n",
   "dev_url": "https://github.com/common-workflow-language/cwltest",
   "doc_url": "http://cwltest.readthedocs.org/",
   "home": "https://github.com/common-workflow-language/cwltest",
   "license": "Apache-2.0",
   "license_file": "LICENSE",
   "summary": "Common workflow language testing framework"
  },
  "build": {
   "entry_points": [
    "cwltest=cwltest.main:main"
   ],
   "number": "0",
   "script": "PYTHON -m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "mr-c"
   ]
  },
  "package": {
   "name": "cwltest",
   "version": "2.5.20240425111257"
  },
  "requirements": {
   "host": [
    "pip",
    "python",
    "setuptools >=45",
    "setuptools_scm >=8.0.4,<9"
   ],
   "run": [
    "python",
    "schema-salad >=5.0.20200220195218,<9",
    "junit-xml >=1.8",
    "pytest >=7,<9",
    "defusedxml"
   ]
  },
  "source": {
   "sha256": "6da11b08de4fed2c6d92c63e7cfb7718ae3b76a3d41609d19ab875f3097974b5",
   "url": "https://pypi.io/packages/source/c/cwltest/cwltest-2.5.20240425111257.tar.gz"
  },
  "test": {
   "commands": [
    "cwltest --help",
    "mkdir -p bin",
    "ln -s $(command -v cwltool) bin/cwl-runner",
    "chmod a+x bin/cwl-runner",
    "PATH=$PATH:$PWD/bin CWLTOOL_OPTIONS=--no-container python -m pytest --pyargs cwltest -p pytester -o pytester_example_dir=tests/test-data -k 'not test_plugin'"
   ],
   "imports": [
    "cwltest",
    "cwltest.tests"
   ],
   "requires": [
    "cwltool",
    "pytest"
   ],
   "source_files": [
    "tests/test-data"
   ]
  }
 },
