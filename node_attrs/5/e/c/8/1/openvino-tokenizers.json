     ],
     "run": [
      "libopenvino"
     ]
    },
    "test": {
     "commands": [
      "test -f $PREFIX/lib/libuser_ov_extensions${SHLIB_EXT}",
      "cmake -S samples -B build -DCMAKE_BUILD_TYPE=Release -G Ninja",
      "cmake --build build --parallel",
      "./build/hello_tokenizers"
