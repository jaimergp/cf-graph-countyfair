   "home": "https://www.sqlfluff.com/",
   "license": "MIT",
   "license_file": "LICENSE.md",
   "summary": "The SQL Linter for Humans"
  },
  "build": {
   "entry_points": [
    "sqlfluff = sqlfluff.cli.commands:cli"
   ],
   "noarch": "python",
   "number": "0",
   "script": "PYTHON -m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
