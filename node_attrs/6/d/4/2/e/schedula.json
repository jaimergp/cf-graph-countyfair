 "raw_meta_yaml": "{% set version = \"1.5.14\" %}\n\n# handle undefined PYTHON in `noarch: generic` outputs\n{% if PYTHON is not defined %}{% set PYTHON = \"$PYTHON\" %}{% endif %}\n\npackage:\n  name: schedula-split\n  version: {{ version }}\n\nsource:\n  - url: https://pypi.io/packages/source/s/schedula/schedula-{{ version }}.tar.gz\n    sha256: bc5e5d53d2eb5a5fa18e9ffaec5a87cfb2f5b99ef37aad108355d096023fe925\n    folder: dist\n  - url: https://github.com/vinci1it2000/schedula/archive/v{{ version }}.tar.gz\n    sha256: 39384418d1e7fb9b63bcffeb884073f9e0d6271c7603c2807b0d72749a433b89\n    folder: src\n\nbuild:\n  noarch: python\n  number: 0\n\nrequirements:\n  host:\n    - python >=3.6\n  run:\n    - python >=3.6\n\ntest:\n  commands: echo tests in subpackages\n\noutputs:\n  - name: schedula\n    build:\n      noarch: python\n      script:\n        - cd dist && {{ PYTHON }} -m pip install . --no-deps -vv --no-build-isolation\n      entry_points:\n        - schedula = schedula.cli:cli\n    requirements:\n      host:\n        - pip\n        - python >=3.6\n      run:\n        - python >=3.6\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n        - which schedula || where schedula\n\n  - name: schedula-with-io\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula', exact=True) }}\n        - dill !=0.2.7\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n\n  - name: schedula-with-web\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula', exact=True) }}\n        - flask\n        - regex\n        - requests\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n\n  - name: schedula-with-parallel\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula', exact=True) }}\n        - multiprocess\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n\n  - name: schedula-with-plot\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula-with-web', exact=True) }}\n        - {{ pin_subpackage('schedula', exact=True) }}\n        - python-graphviz >=0.17\n        - pygments\n        - jinja2\n        - docutils\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n\n  - name: schedula-with-form\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula-with-web', exact=True) }}\n        - {{ pin_subpackage('schedula', exact=True) }}\n        - click-log\n        - click\n        - flask-admin\n        - flask-babel\n        - flask-mail\n        - flask-principal\n        - flask-sqlalchemy\n        - flask-wtf\n        - gunicorn\n        - itsdangerous\n        - rst2txt\n        - sqlalchemy\n        # from flask-security-too[common]\n        - flask-security-too\n        - argon2-cffi >=21.3.0\n        - bcrypt >=4.0.1\n        - bleach >=6.0.0\n        # TODO: build for conda-forge\n        # - stripe\n        # - flask-mailman >=0.3.0\n    test:\n      requires:\n        - pip\n      imports:\n        - schedula\n      commands:\n        - pip check\n        - schedula form --help\n\n  - name: schedula-with-all\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - {{ pin_subpackage('schedula-with-form', exact=True) }}\n        - {{ pin_subpackage('schedula-with-io', exact=True) }}\n        - {{ pin_subpackage('schedula-with-parallel', exact=True) }}\n        - {{ pin_subpackage('schedula-with-plot', exact=True) }}\n        - {{ pin_subpackage('schedula-with-web', exact=True) }}\n        - {{ pin_subpackage('schedula', exact=True) }}\n    test:\n      source_files:\n        - src/examples\n        - src/tests/test_dispatcher.py\n        - src/tests/test_import.py\n        - src/tests/utils\n        - src/setup.cfg\n      requires:\n        - pip\n        # some weird issue\n        - python <3.12\n        # tests-require\n        - coverage\n        - cryptography\n        - ddt\n      imports:\n        - schedula\n      commands:\n        {% set cov = \"coverage run --source=schedula --branch --append -m\" %}\n        # doesn't import without chromedriver stuff\n        - python -c \"__import__('pathlib').Path('src/tests/utils/test_form.py').unlink()\"\n        - pip check\n        # requires `schedula.ext.form` to be importable\n        - schedula --help\n        - schedula form --help\n        - {{ cov }} schedula.cli --help\n        - {{ cov }} schedula.cli form --help\n        - {{ cov }} unittest discover --start-directory src/tests --pattern 'test_*.py'\n        - coverage report --show-missing --skip-covered --fail-under=67\n\nabout:\n  home: https://github.com/vinci1it2000/schedula\n  license: EUPL-1.1\n  license_family: OTHER\n  license_file: src/LICENSE.txt\n  summary: |-\n    Produce a plan that dispatches calls based on a graph of functions, satisfying\n    data dependencies.\n  doc_url: https://schedula.readthedocs.io/en/v{{ version }}\n  doc_source_url: https://github.com/vinci1it2000/schedula/tree/v{{ version }}/doc\n\nextra:\n  feedstock-name: schedula-split\n  recipe-maintainers:\n    - bollwyvl\n",
